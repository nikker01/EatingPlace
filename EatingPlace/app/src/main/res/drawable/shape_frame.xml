<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="2px"
        android:left="2px"
        android:right="2px"
        android:top="2px">
        <shape android:shape="rectangle">

            <solid android:color="#ffbabbbb" />

        </shape>
    </item>

    <item
        android:bottom="3px"
        android:left="3px"
        android:right="3px"
        android:top="3px">

        <shape android:shape="rectangle">

            <span style="white-space: pre;"></span>
            <solid android:color="#ffffff" />

        </shape>

    </item>

</layer-list>  
